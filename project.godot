; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Langr"
run/main_scene="res://Screens/MainMenu.tscn"
config/icon="res://icon.png"
config/quit_on_go_back=false

[autoload]

GlobalVars="*res://Global.gd"
QuestionManager="*res://QuestionManager.gd"

[display]

window/size/width=720
window/size/height=950
window/size/test_height=500
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

theme/custom_font="res://fonts/MainMenuFont.tres"

[input_devices]

pointing/emulate_touch_from_mouse=true

[locale]

test="en"
translations=PoolStringArray( "res://Localization/localization.en.translation", "res://Localization/localization.hu.translation" )

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
